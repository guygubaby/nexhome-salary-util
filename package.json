    "access": "public"
  },
  "scripts": {
    "start": "vite --open",
    "build": "cross-env NODE_ENV=production vite build",
    "preview": "vite preview",
    "lint": "npx eslint ./src --ext .js,.ts,.vue --config=.eslintrc.js"
  },
  "dependencies": {
    "@bryce-loskie/utils": "^0.1.11",
    "@vueuse/core": "^6.0.0",
    "@vueuse/head": "^0.6.0",
    "axios": "^0.21.1",
    "js-cookie": "^3.0.1",
    "mitt": "^3.0.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.0-rc.6",
    "vant": "^3.2.3",
    "vh-check": "^2.0.5",
    "vue": "^3.2.2",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.388",
    "@nexhome/standard": "^0.2.1",
    "@nexhome/yorkie": "^2.0.8",
    "@types/js-cookie": "^2.2.7",
    "@types/node": "^16.4.1",
    "@vitejs/plugin-vue": "^1.2.5",
    "@vue/compiler-sfc": "^3.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^11.1.1",
    "pnpm": "^6.10.3",
    "sass": "^1.36.0",
    "typescript": "^4.3.5",
    "unplugin-icons": "^0.7.4",
    "vite": "^2.4.3",
    "vite-plugin-components": "^0.13.2",
    "vite-plugin-style-import": "^1.2.1",
    "vite-plugin-windicss": "^1.2.7",
    "windicss": "^3.1.6"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node node_modules/@nexhome/standard/lib/git-hooks/verify-commit-msg.js"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --config=.eslintrc.js"
    ]
  }
}
